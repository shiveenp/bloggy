{
  "name": "shiveenp-blog",
  "version": "1.0.0",
  "description": "My blog",
  "type": "module",
  "scripts": {
    "build": "eleventy && npm run sass",
    "serve": "concurrently 'eleventy --serve' 'npm run sass:watch'",
    "sass": "node-sass --output  _site/styles/ styles/",
    "sass:watch": "npm run sass && npm run sass -- -w",
    "clean": "rm -rf _site",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Shiveen Pandita",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "concurrently": "^6.0.0",
    "node-sass": "^5.0.0"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@manustays/eleventy-plugin-generate-social-images": "^4.1.0",
    "sharp": "^0.28.3"
  }
}
